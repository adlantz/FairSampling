   "source": [
    "\n",
    "def dicke_state_list(N,k):\n",
    "\n",
    "    initial = [1] * k + [0] * (N-k)\n",
    "\n",
    "    return list(it.permutations(initial))\n",
    "\n",
    "\n",
    "l = dicke_state_list(5,4)\n",
    "\n"
   ]
